---
import Footer from "../../components/Common/Footer.astro";
import Navbar from "../../components/Common/Navbar.astro";
import PageHead from "../../components/Common/PageHead.astro";
import { PageComponent } from "../../react/PageComponent";

const { token } = Astro.props;
---

<html lang="en" class="scroll-smooth flex flex-col min-h-screen">
  <PageHead />
  <body class="flex flex-col flex-1">
    <Navbar />

    <main class="flex flex-col">
        <PageComponent client:only="react" page="registration" token={token} />
    </main>

    <Footer />
    
  </body>
</html>
